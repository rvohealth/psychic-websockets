{
  "type": "module",
  "name": "@rvoh/psychic-websockets",
  "description": "Websocket system for Psychic applications",
  "version": "2.0.1",
  "author": "RVO Health",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvohealth/psychic-websockets.git"
  },
  "license": "MIT",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "client": "pnpm --cwd=./client start",
    "client:fspec": "cd client && BROWSER=none VITE_PSYCHIC_ENV=test pnpm start",
    "psy": "NODE_ENV=${NODE_ENV:-test} pnpm psy:ts",
    "psy:js": "node ./dist/test-app/src/cli/index.js",
    "psy:ts": "tsx ./test-app/src/cli/index.ts",
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "build:test-app": "rm -rf dist && echo \"building test app to esm...\" && npx tsc -p ./tsconfig.esm.build.test-app.json && echo \"building test app to cjs...\" && npx tsc -p ./tsconfig.cjs.build.test-app.json",
    "uspec": "vitest --config ./spec/unit/vite.config.ts",
    "fspec": "vitest run --config=./spec/features/vite.config.ts",
    "fspec:hanging": "vitest run --config=./spec/features/vite.config.ts --reporter=hanging-process",
    "format": "pnpm prettier . --write",
    "lint": "pnpm eslint --no-warn-ignored \"src/**/*.ts\" && pnpm prettier . --check",
    "dev": "NODE_ENV=development WORKER_COUNT=0 tsx ./test-app/main.ts",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "@rvoh/dream": "^2.0.0",
    "@rvoh/psychic": "^2.0.0",
    "@socket.io/redis-adapter": "*",
    "@socket.io/redis-emitter": "*",
    "ioredis": "*",
    "socket.io": "*",
    "socket.io-adapter": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@rvoh/dream": "^2.0.1",
    "@rvoh/dream-spec-helpers": "^2.0.0",
    "@rvoh/psychic": "^2.0.1",
    "@rvoh/psychic-spec-helpers": "^2.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.1",
    "@types/pg": "^8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.48.1",
    "bullmq": "^5.63.2",
    "eslint": "^9.39.1",
    "express": "^4.22.1",
    "ioredis": "^5.4.1",
    "kysely": "^0.28.7",
    "kysely-codegen": "^0.17.0",
    "luxon-jest-matchers": "^0.1.14",
    "openapi-typescript": "^7.9.1",
    "pg": "^8.13.1",
    "prettier": "^3.3.3",
    "puppeteer": "^24.26.1",
    "socket.io": "^4.8.1",
    "socket.io-adapter": "^2.5.5",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "tslib": "^2.7.0",
    "tsx": "^4.19.3",
    "typedoc": "^0.26.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "pluralize-esm": "^9.0.5",
    "yoctocolors": "^2.1.1"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
