{
  "type": "module",
  "name": "@rvohealth/psychic-websockets",
  "description": "Websocket system for Psychic applications",
  "version": "0.2.0-fgbeta-3",
  "author": "RVOHealth",
  "repository": "https://github.com/rvohealth/psychic-websockets.git",
  "license": "MIT",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "client": "yarn --cwd=./client start",
    "publish": "yarn npm publish",
    "psy": "yarn psyts",
    "psyjs": "node ./dist/test-app/src/cli/index.js",
    "psyts": "node --experimental-specifier-resolution=node --import ./bin/esm.js ./test-app/src/cli/index.ts",
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "uspec": "vitest --config ./spec/unit/vite.config.ts",
    "fspec": "vitest run --config=./spec/features/vite.config.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" && yarn run prettier . --check",
    "dev": "NODE_ENV=development WORKER_COUNT=0 ts-node --transpile-only ./test-app/main.ts",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@socket.io/redis-adapter": "*",
    "@socket.io/redis-emitter": "*",
    "ioredis": "*",
    "socket.io": "*",
    "socket.io-adapter": "*"
  },
  "devDependencies": {
    "@eslint/js": "=9.0.0",
    "@rvohealth/dream": "=0.29.0-fgnewbeta-2",
    "@rvohealth/dream-spec-helpers": "=0.1.0",
    "@rvohealth/psychic": "=0.24.0-fgbeta-10",
    "@rvohealth/psychic-spec-helpers": "=0.2.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/express": "^4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8",
    "@types/supertest": "^6.0.2",
    "bullmq": "^5.12.12",
    "eslint": "^9.9.1",
    "express": "^4.21.2",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "luxon": "^3.5.0",
    "luxon-jest-matchers": "^0.1.14",
    "pg": "^8.13.1",
    "prettier": "^3.3.3",
    "puppeteer": "^24.4.0",
    "socket.io": "^4.8.1",
    "socket.io-adapter": "^2.5.5",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "=7.18.0",
    "vitest": "^3.0.8"
  },
  "packageManager": "yarn@4.4.1"
}
